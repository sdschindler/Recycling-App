<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.recyclingapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.recyclingapp.WhoRUActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.recyclingapp.SignInActivity"
            android:label="@string/title_activity_sign_in" >
        </activity>
        <activity
            android:name="com.recyclingapp.WhatUHaveActivity"
            android:label="@string/title_activity_what_uhave" >
        </activity>
        <activity
            android:name="com.recyclingapp.WhatBottleActivity"
            android:label="@string/title_activity_what_bottle" >
        </activity>
        <activity
            android:name="com.recyclingapp.GlassBottleOneActivity"
            android:label="@string/title_activity_glass_bottle_one" >
        </activity>
        <activity
            android:name="com.recyclingapp.GlassBottleListActivity"
            android:label="@string/title_activity_glass_bottle_list" >
        </activity>
        <activity
            android:name="com.recyclingapp.GlassJarPhotoActivity"
            android:label="@string/title_activity_glass_jar_photo" >
        </activity>
    </application>

</manifest>
